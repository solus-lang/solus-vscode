{
  "name": "solus-lang",
  "publisher": "septumfunk",
  "displayName": "solus",
  "description": "Syntax highlighting for the solus language",
  "repository": {"url": "https://github.com/solus-lang/solus-vscode", "type": "github"},
  "version": "0.4.0",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.106.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "solus",
      "aliases": ["solus", "Solus"],
      "extensions": [".solu", ".solus"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./solu-icon.png",
        "dark": "./solu-icon.png"
      }
    }],
    "grammars": [{
      "language": "solus",
      "scopeName": "source.sol",
      "path": "./syntaxes/solus.tmLanguage.json"
    }]
  }
}
