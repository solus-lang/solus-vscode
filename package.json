{
  "name": "solus-lang",
  "publisher": "septumfunk",
  "displayName": "solus",
  "description": "Syntax highlighting for the solus language",
  "repository": {"url": "https://github.com/septumfunk/solus-lang", "type": "github"},
  "version": "0.4.0",
  "icon": "solus.png",
  "engines": {
    "vscode": "^1.106.2"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "solus",
      "aliases": ["solus", "solus"],
      "extensions": [".sol", ".sol.cfg"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "solus",
      "scopeName": "source.sol",
      "path": "./syntaxes/solus.tmLanguage.json"
    }]
  }
}
